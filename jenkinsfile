pipeline {
    agent any
    stages {
        stage('Prepare') { 
            steps {
                sh 'echo prepare'
                sh 'rm -rf build'
            }
        }
        stage('Build') { 
            steps {
                sh 'mkdir build'
                sh 'cd build'
                sh 'cmake ../src'
                sh 'cmake --build .'
            }
        }
    }
}
